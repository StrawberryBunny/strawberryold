{
  "private": true,
  "engines": {
    "node": ">=0.12.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "browser-sync": "^2.2.1",
    "del": "^1.1.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-babel": "^6.1.1",
    "gulp-cache": "^0.4.2",
    "gulp-cssnano": "^2.0.0",
    "gulp-eslint": "^0.13.2",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.2.1",
    "gulp-load-plugins": "^0.10.0",
    "gulp-plumber": "^1.0.1",
    "gulp-size": "^1.2.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-uglify": "^1.1.0",
    "gulp-useref": "^3.0.0",
    "main-bower-files": "^2.5.0",
    "wiredep": "^2.2.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jquery": true
    },
    "rules": {
      "quotes": [
        0,
        "single"
      ],
      "no-trailing-spaces": 0,
      "no-use-before-define": 0,
      "key-spacing": 0,
      "no-multi-spaces": 0,
      "dot-notation": 0,
      "camelcase": 0,
      "no-unused-vars": 0,
      "space-infix-ops": 0,
      "no-loop-func": 0,      
      "comma-spacing": 0,
      "no-undef": 0,
      "no-redeclare": 0,
      "eqeqeq": 0,
      "no-mixed-spaces-and-tabs": 0,
      "semi": 0,
      "no-shadow": 0,
      "no-spaced-func": 0,
      "curly": 0,
      "no-extra-semi": 0,
      "comma-dangle": 0,
      "space-return-throw-case": 0,
      "no-extend-native": 0,
      "semi-spacing": 0,
      "no-wrap-func": 0,
      "no-shadow-restricted-names": 0,
      "no-extra-boolean-cast": 0,
      "no-empty": 0,
      "no-underscore-dangle": 0,
      "new-cap": 0,
      "no-sequences": 0,
      "no-unused-expression": 0,
      "space-unary-ops": 0,
      "no-unused-expressions": 0
    }
  },
  "dependencies": {
    "cookie_js": "^1.2.0"
  }
}
